#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Nisebase/node_modules/.pnpm/vitepress@1.1.4_@algolia+cl_5ea4e70c9a5c08d1b89570d33df75a2a/node_modules/vitepress/bin/node_modules:/mnt/d/Nisebase/node_modules/.pnpm/vitepress@1.1.4_@algolia+cl_5ea4e70c9a5c08d1b89570d33df75a2a/node_modules/vitepress/node_modules:/mnt/d/Nisebase/node_modules/.pnpm/vitepress@1.1.4_@algolia+cl_5ea4e70c9a5c08d1b89570d33df75a2a/node_modules:/mnt/d/Nisebase/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Nisebase/node_modules/.pnpm/vitepress@1.1.4_@algolia+cl_5ea4e70c9a5c08d1b89570d33df75a2a/node_modules/vitepress/bin/node_modules:/mnt/d/Nisebase/node_modules/.pnpm/vitepress@1.1.4_@algolia+cl_5ea4e70c9a5c08d1b89570d33df75a2a/node_modules/vitepress/node_modules:/mnt/d/Nisebase/node_modules/.pnpm/vitepress@1.1.4_@algolia+cl_5ea4e70c9a5c08d1b89570d33df75a2a/node_modules:/mnt/d/Nisebase/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
